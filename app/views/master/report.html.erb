<h1>Users timetable</h1>

<table border="1">
  <tr>
    <th>User Name</th>
    <th>Task ID</th>
    <th>Date</th>
    <th>Comments</th>
    <th>Hours</th>
  </tr>

<% @time.each do |t| %>
  <tr>
    <td><%= t.user.name %></td>
    <td><%= t.task_id %></td>
    <td><%= t.date %></td>
    <td><%= t.comment %></td>
    <td><%= t.hours %></td>
  </tr>
<% end %>
</table>
<br/>


<br/>
<h1>Users total time</h1>
<table border="1">
  <tr>
    <th>User Name</th>
    <th>Hours</th>
  </tr>

<% @sum.each do |s| %>
  <tr>
    <td><%= s.user.name %></td>
    <td><%= s.summ %></td>
  </tr>
<% end %>
</table>

<br/>
<%= link_to 'Back', master_index_path %>
<h2>Listing timetables for last 7 days</h2>

<table border="1">
  <tr>
    <th>Task ID</th>
    <th>Date</th>
    <th>Comments</th>
    <th>Hours</th>
    <th>Actions</th>
  </tr>

<% @timetables.each do |t| %>
  <tr>
    <td><%= t.task_id %></td>
    <td><%= t.date %></td>
    <td><%= t.comment %></td>
    <td><%= t.hours %></td>
    <td><%= link_to 'Edit', edit_timetable_path(t) %> <%= link_to 'Destroy', t, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Timetable', new_timetable_path %>
